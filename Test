Write-Output ("`nI Drive of PRD - Used Space `n")

foreach ($client in $clients) {
    
    $disk = Get-WmiObject Win32_logicaldisk -ComputerName $server -Filter "DeviceID='I:'"
    $sizeGB = [decimal]($disk.Size / 1gb)
    $freeGB = [decimal]($disk.FreeSpace / 1gb)
    $usedGB = $sizeGB - $freeGB
    Write-Host "$usedGB" -ForegroundColor Green
}
